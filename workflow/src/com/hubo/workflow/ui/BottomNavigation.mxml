<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 width="100%"
		 height="140"
		 horizontalCenter="0"
		 bottom="0">
	
	<fx:Script>
		<![CDATA[
			import com.hubo.workflow.skin.CheckBoxSkin;
			import com.hubo.workflow.util.Global;
		]]>
	</fx:Script>
	
	<s:Rect  id="bg2" height="100%" width="100%" bottom="0" horizontalCenter="0">
		<s:fill>
			<s:SolidColor color="#FFFFFF"/>
		</s:fill>
	</s:Rect>		
	<s:Line xFrom="0" xTo="0" yFrom="0" yTo="0" width="100%" top="38">
		<s:stroke>
			<s:SolidColorStroke color="#A3A3A3" weight="1" caps="square"/>
		</s:stroke>
	</s:Line>	
	<s:Rect  id="bg" top="0" height="38" width="100%">
		<s:fill>
			<s:SolidColor color="#DFDFDF"/>
		</s:fill>
	</s:Rect>		
	<s:Line xFrom="0" xTo="0" yFrom="0" yTo="0" width="100%" top="5">
		<s:stroke>
			<s:SolidColorStroke color="#A3A3A3" weight="1" caps="square"/>
		</s:stroke>
	</s:Line>
	
	<s:Button id="closeButton" skinClass="spark.skins.spark.TitleWindowCloseButtonSkin"
			  width="15" height="15" right="7" top="13" />
	
	<s:Line xFrom="0" xTo="0" yFrom="0" yTo="0" width="100%" bottom="23">
		<s:stroke>
			<s:SolidColorStroke color="#A3A3A3" weight="1" caps="square"/>
		</s:stroke>
	</s:Line>	
	<s:Rect  id="bg3" bottom="0" height="23" width="100%">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="0xF2F4F3"/>
				<s:GradientEntry color="0xEBEBEB"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>	
	
	<s:Group id="log" bottom="6" left="6">
		<s:layout>
			<s:HorizontalLayout/>
		</s:layout>
		<s:BitmapImage source="{Global.LOG_ERROR}"/>
		<s:Label text="10"/>
		<s:BitmapImage source="{Global.LOG_UNCERTAIN}"/>
		<s:Label text="1"/>
	</s:Group>
	
	<s:Label text="日志输出" top="15" left="8"/>
</s:Group>
