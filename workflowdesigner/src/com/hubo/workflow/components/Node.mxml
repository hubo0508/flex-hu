		 xmlns:components="com.hubo.workflow.components.*"
		 useHandCursor="true"
		 buttonMode="true"
		 width="80"
		 height="80">
	
	<fx:Script>
		<![CDATA[
			import mx.core.IVisualElement;
			import mx.events.DynamicEvent;
			
			import spark.primitives.Path;
			
			public function addIcon(icon:IVisualElement):void
			{
				this.addElementAt(icon,2);
			}
			
			public function selectedvisible(value:Boolean):void
			{
				trace("selectedvisible -- value="+value+"    nodeSelected.visible="+nodeSelected.visible);
				if(value != nodeSelected.visible)
				{
					nodeSelected.visible = value;					
				}
			}
			
			
		]]>
	</fx:Script>
	
	<components:NodeSelected id="nodeSelected" visible="false" horizontalCenter="0" verticalCenter="0"/>
	
	<graphic:Ellipse width="42" height="42" horizontalCenter="0" top="8"/>
	
