<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 useHandCursor="true"
		 buttonMode="true"
		 width="100%"
		 height="23"
		 horizontalCenter="0">
	
	<fx:Script>
		<![CDATA[
			import com.workflow.util.BinaryImage;
		]]>
	</fx:Script>
	
	<s:Line xFrom="0" xTo="0" yFrom="0" yTo="0" width="100%" bottom="23">
		<s:stroke>
			<s:SolidColorStroke color="#A3A3A3" weight="1" caps="square"/>
		</s:stroke>
	</s:Line>	
	<s:Rect  id="bg3" bottom="0" height="23" width="100%">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="0xF2F4F3"/>
				<s:GradientEntry color="0xEBEBEB"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>	
	
	<s:Group id="log" bottom="6" left="6">
		<s:layout>
			<s:HorizontalLayout/>
		</s:layout>
		<s:BitmapImage source="{BinaryImage.LOG_ERROR}"/>
		<s:Label text="5"/>
		<s:BitmapImage source="{BinaryImage.LOG_UNCERTAIN}"/>
		<s:Label text="0"/>
	</s:Group>
	
</s:Group>
