<s:Graphic xmlns:fx="http://ns.adobe.com/mxml/2009"
		   xmlns:s="library://ns.adobe.com/flex/spark"
		   xmlns:mx="library://ns.adobe.com/flex/mx"
		   rollOver="rollHandler(event)"
		   rollOut="rollHandler(event)"
		   minHeight="5"
		   minWidth="5">

	<fx:Script>
		<![CDATA[
			
			public var rollMask:Boolean = false;
			
			[Bindable]public var originalColor:uint = 0xFF0000;
			
			[Bindable]public var overColor:uint = 0x000000;
			
			protected function rollHandler(event:MouseEvent):void
			{		
				if(!rollMask) return;
				
				switch(event.type)
				{
					case MouseEvent.ROLL_OUT:
						this.sc.color = originalColor;
						break;
					
					case MouseEvent.ROLL_OVER :
						this.sc.color = overColor;
						break;
					
					default:
						break;
				}
			}

		]]>
	</fx:Script>

	<s:Path data="M 0 0 L 7 0 L 7 1.027 L 6.016 1.027 L 6.016 2.027 L 5.031 2.027 L 5.031 3.012 L 4.016 3.012 L 4.016 4.027 L 3 4.027 L 3 3.027 L 2.016 3.027 L 2.016 2.027 L 1.016 2.027 L 1.016 1.027 L 0 1.027 L 0 0 Z"
			winding="nonZero">		
		<s:fill>
			<s:SolidColor id="sc" color="{originalColor}"/>
		</s:fill>		
	</s:Path>
	
</s:Graphic>
